[gdnative]

singletons=[  ]

[gui]

common/drop_mouse_on_gui_input_disabled=true

[layer_names]

3d_physics/layer_1="Player"
3d_physics/layer_2="Object"
3d_physics/layer_3="Tool"
3d_physics/layer_4="Fire"
3d_physics/layer_5="Landscape"
3d_physics/layer_6="Wall"
3d_physics/layer_7="Construct"
3d_physics/layer_8="Anvil"

[physics]

